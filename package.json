{
  "name": "react-ai-flow",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "clean": "rm -rf packages/*/{*.tsbuildinfo,dist} > /dev/null 2>&1",
    "lint": "biome check",
    "lint:fix": "pnpm lint --write",
    "build": "pnpm clean && tsc --build tsconfig.lib.json",
    "watch": "pnpm build --watch",
    "test": "playwright test",
    "release": "pnpm build && pnpm -r publish --ignore-scripts --no-git-checks",
    "typecheck": "tsc --build",
    "dev": "pnpm --filter react-ai-flow run dev",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@playwright/experimental-ct-react": "^1.51.0",
    "@playwright/test": "^1.51.0",
    "@testing-library/react": "^16.1.0",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^18.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "globals": "^15.9.0",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "postcss": "^8.4.45",
    "react": "^19.1.0",
    "@biomejs/biome": "^1.9.4",
    "react-dom": "^19.1.0",
    "tailwindcss": "^3.4.10",
    "text-stagger": "workspace:^",
    "text-stagger-record": "workspace:^",
    "text-stagger-replay": "workspace:^",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.2.1",
    "vitest": "^2.1.8"
  }
}
